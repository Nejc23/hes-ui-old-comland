    "@progress/kendo-angular-progressbar": "^2.0.1",
    "@progress/kendo-angular-treeview": "^5.3.1",
    "@progress/kendo-angular-upload": "^7.1.1",
    "@progress/kendo-data-query": "^1.5.5",
    "@progress/kendo-drawing": "^1.10.1",
    "@progress/kendo-licensing": "^1.1.4",
    "@progress/kendo-theme-bootstrap": "^4.35.1",
    "@progress/kendo-theme-default": "latest",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.170",
    "angular-cd-timer": "^1.1.0",
    "bootstrap": "^5.0.1",
    "core-js": "^3.8.1",
    "cron-validate": "^1.4.2",
    "cronstrue": "^1.108.0",
    "current-git-branch": "^1.1.0",
    "fs-extra": "^9.0.1",
    "git-describe": "^4.0.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "material-icons": "^0.5.2",
    "moment": "^2.29.1",
    "ng-bootstrap": "^1.6.3",
    "ng-mocks": "^11.2.4",
    "ngx-bootstrap": "^6.2.0",
    "ngx-cookie-service": "~11.0.2",
    "ngx-daterangepicker-material": "^4.0.1",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-tailwind": "^2.1.0",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.6.3",
