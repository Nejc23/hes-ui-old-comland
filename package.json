{
  "name": "angular-template-project",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-production": "ng build --configuration=production && ng build --configuration=production-sl",
    "build-production-dev": "ng build --configuration=production-dev",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage=true",
    "translation-sl": "ng xi18n --output-path assets/i18n --i18n-locale sl && node node_modules/@ngx-translate/i18n-polyfill/extractor/ngx-extractor -i \"src/app/**/**/*.ts\" -f xlf -o src/assets/i18n/messages.xlf && ng run angular-template:xliffmerge:slovenian",
    "translation-en": "ng xi18n --output-path assets/i18n --i18n-locale en && node node_modules/@ngx-translate/i18n-polyfill/extractor/ngx-extractor -i \"src/app/**/**/*.ts\" -f xlf -o src/assets/i18n/messages.xlf && ng run angular-template:xliffmerge:english",
    "pact": "ng test --karmaConfig=src/karma-pact.conf.js --configuration=pact",
    "pact:publish": "node ./pact/publish-pacts.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./version.js && node ./copy-font.js",
    "format:check": "prettier --list-different \"src/app/**/*.ts\"",
    "format:write": "prettier --write \"src/app/**/*.ts\" && prettier --write \"src/debug/**/*.ts\" && prettier --write \"pact/**/*.ts\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/animations": "^9.0.0",
    "@angular/cli": "^9.0.1",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@angular/localize": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@ng-bootstrap/ng-bootstrap": "^5.2.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngx-translate/i18n-polyfill": "~1.0.0",
    "@swimlane/ngx-charts": "^12.1.0",
    "@swimlane/ngx-datatable": "^16.0.3",
    "@types/lodash": "^4.14.149",
    "angular-gridster2": "^8.3.0",
    "bootstrap": "^4.4.1",
    "copy-dir": "^1.2.0",
    "core-js": "^2.6.11",
    "current-git-branch": "^1.1.0",
    "file-saver": "^1.3.8",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.0.4",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ng-bootstrap": "^1.6.3",
    "ng-mocks": "^8.1.0",
    "ngx-cookie-service": "~2.1.0",
    "ngx-daterangepicker-material": "^2.1.11",
    "ngx-i18nsupport": "^0.17.1",
    "node-sass": "^4.13.1",
    "npm-font-open-sans": "^1.1.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@pact-foundation/karma-pact": "^2.3.1",
    "@pact-foundation/pact": "^8.2.6",
    "@pact-foundation/pact-node": "^8.6.2",
    "@pact-foundation/pact-web": "^8.2.6",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "karma-junit-reporter": "1.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "protractor": "^5.4.3",
    "sass-loader": "^7.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pact/**/*.ts": [
      "npm run format:write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run format:write",
      "git add"
    ]
  }
}
