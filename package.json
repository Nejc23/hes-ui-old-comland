{
  "name": "myGrid",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js",
    "start-amera": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --configuration=develop-amera",
    "start-ePointHES": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --configuration=develop-ePointHES",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage=true",
    "pact": "ng test --karmaConfig=src/karma-pact.conf.js --configuration=pact",
    "pact:publish": "node ./pact/publish-pacts.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./version.js",
    "format:check": "prettier --list-different \"src/app/**/*.ts\"",
    "format:write": "prettier --write \"src/app/**/*.ts\" && prettier --write \"src/debug/**/*.ts\" && prettier --write \"pact/**/*.ts\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build-and-extract-i18n": "ng build --prod && npx locl extract -s='dist/hes/*.js' -f=xlf -o='src/assets/i18n/messages.xlf' && xliffmerge --profile xliffmerge.json en sl cs de fr it",
    "build-production:myGrid": "ng build --configuration=production-myGrid",
    "build-production:amera": "ng build --configuration=production-amera",
    "build-production:ePointHES": "ng build --configuration=production-ePointHES"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "~24.0.0",
    "@ag-grid-enterprise/all-modules": "^24.0.0",
    "@ag-grid-enterprise/core": "^24.0.0",
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/animations": "~10.1.4",
    "@angular/cli": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@angular/localize": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@auth0/angular-jwt": "^5.0.1",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngx-translate/i18n-polyfill": "~1.0.0",
    "@progress/kendo-angular-charts": "^4.2.0",
    "@progress/kendo-angular-common": "^1.2.3",
    "@progress/kendo-angular-dateinputs": "^4.3.2",
    "@progress/kendo-angular-dropdowns": "^4.3.2",
    "@progress/kendo-angular-inputs": "^6.7.2",
    "@progress/kendo-angular-intl": "^2.0.3",
    "@progress/kendo-angular-l10n": "^2.0.2",
    "@progress/kendo-angular-popup": "^3.0.6",
    "@progress/kendo-angular-upload": "^6.0.2",
    "@progress/kendo-data-query": "^1.5.4",
    "@progress/kendo-drawing": "^1.9.2",
    "@progress/kendo-theme-bootstrap": "^4.21.1",
    "@progress/kendo-theme-default": "latest",
    "@types/jquery": "^3.5.2",
    "@types/lodash": "^4.14.161",
    "angular-gridster2": "^10.1.6",
    "bootstrap": "^4.5.2",
    "core-js": "^2.5.4",
    "current-git-branch": "^1.1.0",
    "fs-extra": "^8.1.0",
    "git-describe": "^4.0.4",
    "hammerjs": "^2.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "ng-bootstrap": "^1.6.3",
    "ng-mocks": "^10.2.0",
    "ngx-bootstrap": "^6.1.0",
    "ngx-cookie-service": "~2.1.0",
    "ngx-i18nsupport": "^0.17.1",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.6.3",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@ag-grid-community/core": "^24.0.0",
    "@pact-foundation/karma-pact": "^2.2.0",
    "@pact-foundation/pact": "^8.2.6",
    "@pact-foundation/pact-node": "^8.6.2",
    "@pact-foundation/pact-web": "^8.2.6",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "~3.4",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "@locl/cli": "^1.0.0",
    "codelyzer": "^5.2.2",
    "husky": "^3.0.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "1.2.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "tslint-config-prettier": "^1.18.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pact/**/*.ts": [
      "npm run format:write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run format:write",
      "git add"
    ]
  }
}
