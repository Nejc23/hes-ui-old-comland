    "tailwindcss": "^2.1.1",
    "tslib": "^2.0.3",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@ag-grid-community/core": "^24.1.0",
    "@locl/cli": "^1.0.0",
    "@pact-foundation/karma-pact": "^2.3.1",
    "@pact-foundation/pact": "^9.13.0",
    "@pact-foundation/pact-node": "^10.11.0",
    "@pact-foundation/pact-web": "^9.13.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jscrambler": "^5.5.25",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "2.0.1",
    "lint-staged": "^10.5.3",
    "postcss": "^8.3.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pact/**/*.ts": [
      "npm run format:write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run format:write",
      "git add"
    ]
  }
}
