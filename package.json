{
  "name": "myGrid",
  "version": "2.3.8",
  "scripts": {
    "ng": "ng",
    "start": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.crt --proxy-config proxy.conf.js",
    "start-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js",
    "start-sl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --configuration=develop-sl",
    "start-amera": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.crt --proxy-config proxy.conf.js --configuration=develop-amera",
    "start-amera-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --configuration=develop-amera",
    "start-ePointHES": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.crt --proxy-config proxy.conf.js --configuration=develop-ePointHES",
    "start-ePointHES-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --configuration=develop-ePointHES",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage=true",
    "pact": "ng test --karmaConfig=src/karma-pact.conf.js --configuration=pact",
    "pact:publish": "node ./pact/publish-pacts.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./version.js",
    "format:check": "prettier --list-different \"src/app/**/*.ts\"",
    "format:write": "prettier --write \"src/app/**/*.ts\" && prettier --write \"src/debug/**/*.ts\" && prettier --write \"pact/**/*.ts\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build-and-extract-i18n": "ng build --prod && npx locl extract -s='dist/hes/*.js' -f=xlf -o='src/assets/i18n/messages.xlf' && xliffmerge --profile xliffmerge.json en sl cs de fr it",
    "build-production:myGrid": "ng build --configuration=production-myGrid",
    "build-production:amera": "ng build --configuration=production-amera",
    "build-production:ePointHES": "ng build --configuration=production-ePointHES"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "~24.1.0",
    "@ag-grid-enterprise/all-modules": "^24.1.0",
    "@ag-grid-enterprise/core": "^24.1.0",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/animations": "~11.0.4",
    "@angular/cli": "~11.0.4",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/language-service": "~11.0.4",
    "@angular/localize": "~11.0.4",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-loading-bar/core": "^5.1.1",
    "@ngx-loading-bar/http-client": "^5.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@progress/kendo-angular-charts": "^4.2.1",
    "@progress/kendo-angular-common": "^1.0.0",
    "@progress/kendo-angular-dateinputs": "^4.4.1",
    "@progress/kendo-angular-dropdowns": "^4.4.1",
    "@progress/kendo-angular-inputs": "^6.7.4",
    "@progress/kendo-angular-intl": "^2.0.4",
    "@progress/kendo-angular-l10n": "^2.0.0",
    "@progress/kendo-angular-layout": "^5.1.0",
    "@progress/kendo-angular-popup": "^3.0.7",
    "@progress/kendo-angular-progressbar": "^0.2.3",
    "@progress/kendo-angular-upload": "^6.1.0",
    "@progress/kendo-data-query": "^1.5.4",
    "@progress/kendo-drawing": "^1.9.3",
    "@progress/kendo-theme-bootstrap": "^4.27.0",
    "@progress/kendo-theme-default": "latest",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.165",
    "angular-gridster2": "^11.0.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.8.1",
    "cron-validate": "^1.4.2",
    "cronstrue": "^1.108.0",
    "current-git-branch": "^1.1.0",
    "fs-extra": "^9.0.1",
    "git-describe": "^4.0.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "material-icons": "^0.5.2",
    "moment": "^2.29.1",
    "ng-bootstrap": "^1.6.3",
    "ng-mocks": "^11.2.4",
    "ngx-bootstrap": "^6.2.0",
    "ngx-cookie-service": "~11.0.2",
    "ngx-i18nsupport": "^0.17.1",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.6.3",
    "stream": "0.0.2",
    "tslib": "^2.0.3",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@ag-grid-community/core": "^24.1.0",
    "@locl/cli": "^1.0.0",
    "@pact-foundation/karma-pact": "^2.3.1",
    "@pact-foundation/pact": "^9.13.0",
    "@pact-foundation/pact-node": "^10.11.0",
    "@pact-foundation/pact-web": "^9.13.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "2.0.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pact/**/*.ts": [
      "npm run format:write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run format:write",
      "git add"
    ]
  }
}
