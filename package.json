{
  "name": "myGrid",
  "version": "v5.6.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.pem ",
    "start-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve",
    "start-sl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.pem --configuration=develop-sl",
    "start-de": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.pem --configuration=develop-de",
    "start-amera": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.pem --configuration=develop-amera",
    "start-amera-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve  --configuration=develop-amera",
    "start-ePointHES": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --ssl --ssl-key ./resources/ssl/localhost/localhost.key --ssl-cert ./resources/ssl/localhost/localhost.pem --configuration=develop-ePointHES",
    "start-ePointHES-nossl": "node --max-http-header-size=100000 ./node_modules/@angular/cli/bin/ng serve --configuration=develop-ePointHES",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node ./version.js",
    "format:check": "prettier --list-different \"src/app/**/*.ts\"",
    "format:write": "prettier --write \"src/app/**/*.ts\" && prettier --write \"src/debug/**/*.ts\"",
    "build-production:myGrid": "ng build --configuration=production-myGrid",
    "build-production:amera": "ng build --configuration=production-amera",
    "build-production:ePointHES": "ng build --configuration=production-ePointHES",
    "build-production:myGrid:jscr": "ng build --configuration=production-myGrid && jscrambler",
    "build-production:amera:jscr": "ng build --configuration=production-amera && jscrambler",
    "build-production:ePointHES:jscr": "ng build --configuration=production-ePointHES && jscrambler",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production",
    "ng-openapi-gen": "ng-openapi-gen"
  },
  "private": true,
