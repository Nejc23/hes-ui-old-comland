{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "advance-hes": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/advance-hes",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "es5BrowserSupport": true,
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            }
          },
          "configurations": {
            "develop": {
              "i18nFile": "src/assets/i18n/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "fileReplacements": [                
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/default/favicon.ico"
                }
              ]
            },
            "develop-sl": {
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/locale.ts",
                  "with": "src/environments/locale-sl.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/default/favicon.ico"
                }
              ]
            },
            "production": {
              "outputPath": "dist/advance-hes/en",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/default/favicon.ico"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "baseHref": "/en/"
            },
            "production-sl": {
              "outputPath": "dist/advance-hes/sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/locale.ts",
                  "with": "src/environments/locale-sl.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/default/favicon.ico"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "baseHref": "/sl/"
            },
            "production-dev": {
              "outputPath": "c:/inetpub/wwwroot/en",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "baseHref": "/en/"
            },

            "develop-amera": {
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/amera/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              }
            },
            "develop-ePointHES": {
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/ePointHES/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              }
            },

            "production-amera": {
              "outputPath": "dist/amera/en",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/amera/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "baseHref": "/en/"
            },

            "production-ePointHES": {
              "outputPath": "dist/ePointHES/en",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/ePointHES/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "baseHref": "/en/"
            },

            "production-sl-amera": {
              "outputPath": "dist/amera/sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                },
                {
                  "replace": "src/environments/locale.ts",
                  "with": "src/environments/locale-sl.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/amera/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "baseHref": "/sl/"
            },

            "production-sl-ePointHES": {
              "outputPath": "dist/ePointHES/sl",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                },
                {
                  "replace": "src/environments/locale.ts",
                  "with": "src/environments/locale-sl.ts"
                },
                {
                  "replace": "src/favicon.ico",
                  "with": "src/environments/brand/ePointHES/favicon.ico"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                }
              ],
              "i18nFile": "src/assets/i18n/messages.sl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "sl",
              "baseHref": "/sl/"
            }

          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "advance-hes:build:develop"
          },
          "configurations": {
            "production": {
              "browserTarget": "advance-hes:build:production"
            },
            "production-amera": {
              "browserTarget": "advance-hes:build:production-amera"
            },
            "production-ePointHES": {
              "browserTarget": "advance-hes:build:production-ePointHES"
            },
            "develop-sl": {
              "browserTarget": "advance-hes:build:develop-sl"
            },
            "develop-amera": {
              "browserTarget": "advance-hes:build:develop-amera"
            },
            "develop-ePointHES": {
              "browserTarget": "advance-hes:build:develop-ePointHES"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "advance-hes:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            }
          },
          "configurations": {
            "pact": {
              "main": "src/test-pact.ts",
              "karmaConfig": "src/karma-pact.conf.js",
              "tsConfig": "src/tsconfig-pact.spec.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {},
          "configurations": {
            "slovenian": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "sl",
                "languages": [
                  "sl"
                ],
                "useSourceAsTarget": true
              }
            },
            "english": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "en",
                "languages": [
                  "en"
                ],
                "useSourceAsTarget": true
              }
            }
          }
        }
      }
    },
    "advance-hes-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "advance-hes:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "advance-hes:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "advance-hes",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
