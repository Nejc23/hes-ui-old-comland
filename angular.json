{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "myGrid": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/myGrid",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            },
            "allowedCommonJsDependencies": [
              "jquery",
              "lodash",
              "@progress/kendo-angular-intl/locales/sl/all",
              "@progress/kendo-angular-intl/locales/cs/all",
              "@progress/kendo-angular-intl/locales/de/all",
              "@progress/kendo-angular-intl/locales/fr/all",
              "@progress/kendo-angular-intl/locales/it/all",
              "@progress/kendo-angular-intl/locales/en-GB/all",
              "cronstrue/i18n",
              "cron-validate"
            ]
          },
          "configurations": {
            "develop": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "develop-amera": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/amera/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              }
            },
            "develop-ePointHES": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/ePointHES/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              }
            },
            "production": {
              "outputPath": "dist/hes",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/default/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-myGrid": {
              "outputPath": "dist/myGrid",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/default/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-amera": {
              "outputPath": "dist/amera",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/amera/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-ePointHES": {
              "outputPath": "dist/ePointHES",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/ePointHES/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "myGrid:build:develop"
          },
          "configurations": {
            "production": {
              "browserTarget": "myGrid:build:production"
            },
            "production-amera": {
              "browserTarget": "myGrid:build:production-amera"
            },
            "production-ePointHES": {
              "browserTarget": "myGrid:build:production-ePointHES"
            },
            "develop-amera": {
              "browserTarget": "myGrid:build:develop-amera"
            },
            "develop-ePointHES": {
              "browserTarget": "myGrid:build:develop-ePointHES"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "myGrid-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "myGrid:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "myGrid:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "e2e//**/*.ts",
              "e2e//**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "myGrid",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
