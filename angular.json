{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "myGrid": {
      "i18n": {
        "locales": {
          "en": "src/assets/i18n/messages.en.xlf",
          "sl": "src/assets/i18n/messages.sl.xlf",
          "cs": "src/assets/i18n/messages.cs.xlf",
          "de": "src/assets/i18n/messages.de.xlf",
          "fr": "src/assets/i18n/messages.fr.xlf",
          "it": "src/assets/i18n/messages.it.xlf"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "localize": false,
            "aot": true,
            "outputPath": "dist/myGrid",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            },
            "allowedCommonJsDependencies": [
              "jquery",
              "lodash",
              "@progress/kendo-angular-intl/locales/sl/all",
              "@progress/kendo-angular-intl/locales/cs/all",
              "@progress/kendo-angular-intl/locales/de/all",
              "@progress/kendo-angular-intl/locales/fr/all",
              "@progress/kendo-angular-intl/locales/it/all",
              "@progress/kendo-angular-intl/locales/en-GB/all",
              "cronstrue/i18n",
              "cron-validate"
            ]
          },
          "configurations": {
            "develop": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "localize": [
                "sl"
              ]
            },
            "develop-amera": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/amera/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              },
              "localize": [
                "en"
              ]
            },
            "develop-ePointHES": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/ePointHES/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              },
              "localize": [
                "en"
              ]
            },
            "develop-sl": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "localize": [
                "sl"
              ]
            },
            "production": {
              "outputPath": "dist/hes",
              "deleteOutputPath": true,
              "localize": false,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/default/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-myGrid": {
              "localize": [
                "en",
                "sl",
                "cs",
                "de",
                "fr",
                "it"
              ],
              "outputPath": "dist/myGrid",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/default/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-amera": {
              "localize": [
                "en",
                "sl",
                "cs",
                "de",
                "fr",
                "it"
              ],
              "outputPath": "dist/amera",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/amera/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/amera/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/amera"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production-ePointHES": {
              "localize": [
                "en",
                "sl",
                "cs",
                "de",
                "fr",
                "it"
              ],
              "outputPath": "dist/ePointHES",
              "deleteOutputPath": true,
              "assets": [
                "src/assets",
                {
                  "glob": "favicon.ico",
                  "input": "src/environments/brand/ePointHES/",
                  "output": "/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/brand/default/brand.ts",
                  "with": "src/environments/brand/ePointHES/brand.ts"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/brand/ePointHES"
                ]
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "13mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "myGrid:build:develop"
          },
          "configurations": {
            "production": {
              "browserTarget": "myGrid:build:production"
            },
            "production-amera": {
              "browserTarget": "myGrid:build:production-amera"
            },
            "production-ePointHES": {
              "browserTarget": "myGrid:build:production-ePointHES"
            },
            "develop-sl": {
              "browserTarget": "myGrid:build:develop-sl"
            },
            "develop-amera": {
              "browserTarget": "myGrid:build:develop-amera"
            },
            "develop-ePointHES": {
              "browserTarget": "myGrid:build:develop-ePointHES"
            },
            "sl": {
              "browserTarget": "myGrid:build:sl"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "myGrid:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
              },
              "src/styles.scss"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/brand/default"
              ]
            }
          },
          "configurations": {
            "pact": {
              "main": "src/test-pact.ts",
              "karmaConfig": "src/karma-pact.conf.js",
              "tsConfig": "src/tsconfig-pact.spec.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {},
          "configurations": {
            "slovenian": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "sl",
                "languages": [
                  "sl"
                ],
                "useSourceAsTarget": true
              }
            },
            "english": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "en",
                "languages": [
                  "en"
                ],
                "useSourceAsTarget": true
              }
            },
            "czech": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "cs",
                "languages": [
                  "cs"
                ],
                "useSourceAsTarget": true
              }
            },
            "deutch": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "de",
                "languages": [
                  "de"
                ],
                "useSourceAsTarget": true
              }
            },
            "french": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "fr",
                "languages": [
                  "fr"
                ],
                "useSourceAsTarget": true
              }
            },
            "italian": {
              "xliffmergeOptions": {
                "i18nFormat": "xlf",
                "srcDir": "src/assets/i18n",
                "genDir": "src/assets/i18n",
                "defaultLanguage": "it",
                "languages": [
                  "it"
                ],
                "useSourceAsTarget": true
              }
            }
          }
        }
      }
    },
    "myGrid-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "myGrid:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "myGrid:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "myGrid",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  }
}
